data List a 
  = Nil 
  | Cons
    { head :: a
    ; tail :: List a
    }

null = \l -> case l of
  { Nil -> true
  ; _ -> false
  }

concat = \l1 -> \l2 -> case l1 of 
  { Nil -> l2
  ; Cons x -> Cons(
    { head: x.head
    , tail: concat(x.tail)(l2) 
    })
  }

flatten = \xss -> case xss of 
  { Nil -> Nil
  ; Cons cons -> concat(cons.head)(flatten(cons.tail))
  }
